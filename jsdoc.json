{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc","closure"]
  },
  "recurseDepth": 10,
  "source": {
    "include": ["src/MiniSearch.js", "src/SearchableMap/SearchableMap.js", "MiniSearch.svg"]
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash/",
    "readme": "README.md",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "menu": {
      "GitHub Repository": {
        "href":"https://github.com/lucaong/minisearch",
        "target":"_blank",
        "class":"menu-item",
        "id":"github_repo_link"
      }
    }
  }
}
